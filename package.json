{
  "name": "react-tailwind-vite-kit",
  "version": "1.0.0",
  "description": "A React boilerplate with Tailwind CSS, NextUI, Vite, and Husky pre-configured for rapid development.",
  "scripts": {
    "prepare": "husky install",
    "dev": "concurrently \"vite\" \"npm run watch:css\"",
    "build": "vite build",
    "serve": "vite preview",
    "watch:css": "tailwindcss -i ./css/tailwind.css -o ./css/style.css --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "author": "Vedat Aydın Uğur",
  "license": "MIT",
  "dependencies": {
    "@nextui-org/react": "^2.6.8",
    "axios": "^1.7.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "husky": "^8.0.3",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
